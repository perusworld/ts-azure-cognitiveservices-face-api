{
  "name": "ts-azure-cognitiveservices-face-api",
  "version": "1.1.0",
  "description": "Node.js TypeScript Module - Azure Cognitive Services Face API",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "test": "jest",
    "build": "node ./node_modules/typescript/bin/tsc"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "transform": {
      "^.+\\.(ts|tsx|js|html)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/perusworld/ts-azure-cognitiveservices-face-api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "module",
    "azure",
    "cognitiveservices",
    "fae-api"
  ],
  "author": "Saravana Perumal Shanmugam <saravanaperumal@msn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/perusworld/ts-azure-cognitiveservices-face-api/issues"
  },
  "homepage": "https://github.com/perusworld/ts-azure-cognitiveservices-face-api",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.0",
    "@types/request": "^2.47.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "request": "^2.86.0"
  }
}
